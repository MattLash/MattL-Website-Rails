            </div><!--  /resume-year -->
            <div class="resume-btn">
              <a href="#experience4" data-toggle="collapse" data-parent="#experience" class="collapsed"><span class="glyphicon glyphicon-menu-right" aria-hidden="true"></span></a>
            </div>
          </div>
          <div class="resume-panel wow fadeInRight">
            <div class="panel-heading">
